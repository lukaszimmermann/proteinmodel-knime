<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

    This is an example for providing an own category 
      <extension point="org.knime.workbench.repository.categories">
      <category description="Category containing nodes from the Proteinevolution department" level-id="protevo" name="Proteinevolution" path="/"/>
      
      <category description="Nodes for Data input" level-id="input" name="Input" path="/protevo"/>
      
      <category description="Nodes for visualization" level-id="viz" name="Visualization" path="/protevo"/>
      <category description="Nodes for indexing against databases" level-id="idx" name="Index" path="/protevo"/>
      <category description="Nodes for indexing against databases" level-id="analysis" name="Analysis" path="/protevo"/>
   	</extension>
    
   <extension point="org.knime.workbench.repository.nodes">
<node category-path="/protevo/input" factory-class="org.proteinevolution.nodes.input.xquestreader.XQuestReaderNodeFactory" id="org.proteinevolution.XQuestReaderNodeFactory"/>
<node category-path="/protevo/idx" factory-class="org.proteinevolution.nodes.index.fastaindexer.FastaIndexerNodeFactory" id="org.proteinevolution.fastaindexer.FastaIndexerNodeFactory"/>
<node category-path="/protevo/viz" factory-class="org.proteinevolution.nodes.visualization.structureviewer.StructureViewerNodeFactory" id="org.proteinevolution.structureviewer.StructureViewerNodeFactory"/>
<node category-path="/protevo/input" factory-class="org.proteinevolution.nodes.input.pdbatomreader.PDBATOMReaderNodeFactory" id="org.proteinevolution.pdb.pdbatomreader.PDBATOMReaderNodeFactory"/>
<node category-path="/protevo/input" factory-class="org.proteinevolution.nodes.input.plinkreader.PLinkReaderNodeFactory" id="org.proteinevolution.plinkreader.PLinkReaderNodeFactory"/>
<node category-path="/protevo/input" factory-class="org.proteinevolution.nodes.input.pdbdirectoryreader.PDBDirectoryReaderNodeFactory" id="org.proteinevolution.pdbdirreader.PDBDirReaderNodeFactory"/>
<node category-path="/protevo/analysis" factory-class="org.proteinevolution.nodes.analysis.crosslinkpredictor.CrossLinkPredictorNodeFactory" id="org.proteinevolution.nodes.crosslinkpredictor.CrossLinkPredictorNodeFactory"/>
<node category-path="/protevo/idx" factory-class="org.proteinevolution.nodes.index.gridbuilder.GridBuilderNodeFactory" id="org.proteinevolution.nodes.input.pdb.gridbuilder.GridBuilderNodeFactory"/>
<node category-path="/" factory-class="org.proteinevolution.nodes.hhsuite.hhblits.HHblitsNodeFactory" id="org.proteinevolution.nodes.hhsuite.hhblits.HHblitsNodeFactory"/>
<node category-path="/protevo/input" factory-class="org.proteinevolution.nodes.input.alignmentreader.AlignmentReaderNodeFactory" id="org.proteinevolution.nodes.input.alignmentreader.AlignmentReaderNodeFactory"/>
<node category-path="/protevo/analysis" factory-class="org.proteinevolution.nodes.analysis.alignmentinfo.AlignmentInfoNodeFactory" id="org.proteinevolution.nodes.analysis.alignmentinfo.AlignmentInfoNodeFactory"/>
<node category-path="/" factory-class="org.proteinevolution.nodes.output.alignmentwriter.AlignmentWriterNodeFactory" id="org.proteinevolution.nodes.output.alignmentwriter.AlignmentWriterNodeFactory"/>
</extension>
   
   
<!-- Preference Page for setting the HH-suite build directory -->

 <extension point="org.eclipse.ui.preferencePages">
      <page class="org.proteinevolution.preferences.hhsuite.HHSuitePreferencePage" id="org.proteinevolution.preferences.hhsuite.preferencespage" name="HH-Suite">
      </page>
   </extension>
   
   
  <extension point="org.knime.core.PortType">
  <portType color="#FF0000" hidden="false" name="Sequence Alignment" objectClass="org.proteinevolution.models.knime.alignment.SequenceAlignmentPortObject" objectSerializer="org.proteinevolution.models.knime.alignment.SequenceAlignmentPortObject$Serializer" specClass="org.proteinevolution.models.knime.alignment.SequenceAlignmentPortObjectSpec" specSerializer="org.proteinevolution.models.knime.alignment.SequenceAlignmentPortObjectSpec$Serializer"/>
  </extension> 
   
  <extension point="org.knime.core.DataType">
  
  <DataType cellClass="org.proteinevolution.models.knime.alignment.SequenceAlignmentCell">
  <serializer cellClass="org.proteinevolution.models.knime.alignment.SequenceAlignmentCell" serializerClass="org.proteinevolution.models.knime.alignment.SequenceAlignmentCell$SequenceAlignmentCellSerializer"/>
  </DataType>   


  
  </extension> 
   
   
</plugin>
